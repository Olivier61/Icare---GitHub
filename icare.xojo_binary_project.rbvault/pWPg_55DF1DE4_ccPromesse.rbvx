  sorc/d: (Enco/N: 134217984
		Function validate() As Boolean
			Dim err As Boolean = False
			
			errNom.Visible = False
			errMail.Visible = False
			errMontant.Visible = False
			
			bSave.Enabled = True
			
			if trim(txtNom.Text) = "" then
				err = True
				errNom.Visible = True
			end if
			
			if trim(txtMail.Text) = "" or not checkMail(txtMail.Text) then
				err = True
				errMail.Visible = True
			end if
			
			if CDbl(txtMontant.Text) < 1 then
				err = True
				errMontant.Visible = True
			end if
			
			return err
			
			
			
			
			
			
			
			
		End Function
  )
