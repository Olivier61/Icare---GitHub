		Sub Action()
			if not validate then
				
				'ccMain.ccListPromesses1.Visible = False
				'ccMain.ccListPromesses1.Visible = False
				'main.ccMain1.ccListPromesses1.Visible = False
				Dim today As New Date
				Dim dr As  New DatabaseRecord
				Dim lastID As Integer
				
				txtNom.Text = Titlecase(trim(txtNom.Text))
				txtMail.Text = Lowercase(trim(txtMail.Text))
				
				dr.Column("Nom") = txtNom.Text
				dr.Column("Mail") = txtMail.Text
				dr.DoubleColumn("Montant") = CDbl(txtMontant.Text)
				
				if txtAnonyme.Value = True then
					dr.IntegerColumn("Anonyme") = 1
				else
					dr.IntegerColumn("Anonyme") = 0
				end
				dr.DateColumn("Date") = today
				dr.IntegerColumn("Paye") = 0
				dr.DoubleColumn("MontantPaye") = 0
				dr.DateColumn("DatePaye") = Nil
				dr.IntegerColumn("PayementID") = 0
				
				Session.db.InsertRecord("Promesse",dr)
				lastID = Session.db.LastRowID
				'Session.db.Commit
				
				if Session.db.Error then
					MsgBox("erreur écriture " + session.db.ErrorMessage )
				end if
				
				'ccMain.ccListPromesses1.Visible = True
				'ccMain.ccListPromesses1.Visible = True
				'Main.ccMain1.ccListPromesses1.lstProm.buildProm
				'main.ccMain1.ccListPromesses1.Visible = True
				// Send Mail
				Session.sendMailProm (txtNom.Text, txtMontant.Text, txtAnonyme.Value, lastID , txtMail.Text)
				
				Session.sUpdated = True
				
				// Close 
				Self.Close
				Parent.Close
				clearPromesse
				
				dialogMerci1.Show
				
			end if
			
		End Sub
   )
